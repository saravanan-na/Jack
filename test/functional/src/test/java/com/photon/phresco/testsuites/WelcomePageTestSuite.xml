<?xml version="1.0" encoding="UTF-8"?>
<suite name="WelcomePageTestSuite" parallel="tests"
	thread-count="5" verbose="3">

      <test name="VerifyWelcomePageOnFirefox" junit="false"
		preserve-order="true">
		<parameter name="jquerywidgetdataXml" value="/JQueryWidgetData.xml" />
		<parameter name="browser" value="firefox" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="com.photon.phresco.testcases.WelcomePageTestCase" />
		</classes>
	</test> 

	<test name="VerifyWelcomePageOngooglechrome" junit="false"
		preserve-order="true">
		<parameter name="jquerywidgetdataXml" value="/JQueryWidgetData.xml" />
		<parameter name="browser" value="googlechrome" />
		<parameter name="platform" value="WINDOWS" />
		<classes>
			<class name="com.photon.phresco.testcases.WelcomePageTestCase" />
		</classes>
	</test> 
	
		
</suite>
 

 	
 
