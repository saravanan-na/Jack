<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mojos>
    <mojo>
        <goal>package</goal>
        <description>Goal which packages the HTML5-Jquery project</description>
        <requiresDirectInvocation>false</requiresDirectInvocation>
        <requiresProject>true</requiresProject>
        <requiresReports>false</requiresReports>
        <aggregator>false</aggregator>
        <requiresOnline>false</requiresOnline>
        <inheritedByDefault>true</inheritedByDefault>
        <implementation>
           <dependency>
               <groupId>com.photon.phresco.plugins</groupId>
               <artifactId>java-phresco-plugin</artifactId>
               <type>jar</type>
               <version>3.2.0.8003</version>
               <class>com.photon.phresco.plugins.java.JavaPlugin</class>
           </dependency>
       </implementation>
        <language>java</language>
        <instantiationStrategy>per-lookup</instantiationStrategy>
        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Name</value>
                    </name>
                    <type>String</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildName</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Number</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Show Settings</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>showSettings</key>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value>false</value>
                    <dependency>environmentName</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
								<groupId>com.photon.phresco.commons</groupId>
								<artifactId>phresco-commons</artifactId>
								<type>jar</type>
								<version>3.2.0.8003</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
					<multiple>true</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
				<parameter>
                    <pluginParameter>plugin</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>true</key>
                            <value>-DskipTests=true</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>false</key>
                            <value>-DskipTests=false</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Skip Unit Test</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>skipTest</key>
					<multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value>false</value>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value></value>
                </parameter>
				<parameter>
					<pluginParameter>plugin</pluginParameter>
						<mavenCommands>
							<mavenCommand>
								<key>true</key>
								<value>-Dmaven.yuicompressor.skip=true</value>
							</mavenCommand>
						<mavenCommand>
							<key>false</key>
							<value>-Dmaven.yuicompressor.skip=false</value>
						</mavenCommand>
						</mavenCommands>
					<name>
						<value lang="en">Minify</value>
					</name>
					<type>Hidden</type>
					<required>false</required>
					<editable>true</editable>
					<description></description>
					<key>minify</key>
					<multiple>false</multiple>
					<value>true</value>
					<sort>false</sort>
					<show>false</show>
				</parameter>
				<parameter>
					<name>
                        <value lang="en">Pack Minified Files</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>packMinifiedFiles</key>
                    <multiple>false</multiple>
					<sort>false</sort>
					<show>true</show>
                    <value>false</value>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Build Type</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>packageType</key>
                    <possibleValues>
                        <value key="war">war</value>
                        <value key="zip">zip</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <value>zip</value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
				<parameter>
                    <name>
                        <value lang="en">Package File Browse</value>
                    </name>
                    <type>packageFileBrowse</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>packageFileBrowse</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>false</sort>
                    <show>true</show>
                </parameter>
            </parameters>
        </configuration>
    </mojo>
</mojos>